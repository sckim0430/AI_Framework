{
    "model": {
        "type": "AlexNet",
        "backbone": {
            "type": "AlexNet",
            "in_channel": 3,
            "lrn_param": [
                5,
                1e-4,
                0.75,
                2.0
            ],
            "pretrained": false,
            "init_weight": true
        },
        "cls_head": {
            "type": "AlexNet_Head",
            "in_channel": 256,
            "num_class": 1000,
            "dropout_ratio": 0.5,
            "pooling_type": "avg",
            "loss_cls": {
                "type": "CrossEntropyLoss",
                "loss_weight":1.0,
                "class_weight":null
            },
            "init_weight": true
        }
    },
    "optimizer": {
        "type": "SGD",
        "learning_rate": 0.01,
        "momentum": 0.9,
        "weight_decay": 5e-4
    }
}